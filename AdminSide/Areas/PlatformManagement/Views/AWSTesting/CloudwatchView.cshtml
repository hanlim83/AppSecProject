@model Amazon.CloudWatchLogs.Model.GetLogEventsResponse;
@{
    ViewData["Title"] = "Testing Page";
}

<div class="row">
    <div class="col-lg-3">
        <partial name="../_AWSTestingNav.cshtml" />
    </div>
    <div class="col-lg-9" style="margin-top: 25px; margin-bottom: 10%;">
        <h4>Content</h4>
        <div class="row">
            <div class="col-lg-6">
                <table class="table">
                    <thead>
                        <tr>
                            <th>
                                TimeStamp
                            </th>
                            <th>
                                Message
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var loggedEvent in Model.Events)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => loggedEvent.Timestamp)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => loggedEvent.Message)
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
